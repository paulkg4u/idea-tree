{
  "name": "idea_tree",
  "version": "1.0.0",
  "description": "sample app to store ideas and thoughts in a local excel sheet",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icon.png --prune=true --out=release-builds",
    "package-win": "electron-packager . idea-tree --overwrite --asar=true --platform=win32 --arch=ia32 --icon=./assets/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Idea Tree\"",
    "package-win64": "electron-packager . idea-tree --overwrite --asar=true --platform=win32 --arch=x64 --icon=./assets/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Idea Tree\"",
    "package-linux": "electron-packager . idea-tree --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icon.png --prune=true --out=release-builds"
  },
  "author": "paulkg4u",
  "license": "MIT",
  "dependencies": {
    "@uirouter/angularjs": "^1.0.24",
    "angular": "^1.7.9",
    "angular-animate": "^1.7.9",
    "angular-aria": "^1.7.9",
    "angular-material": "^1.1.21",
    "exceljs": "^3.8.0",
    "jquery": "^3.4.1",
    "ngstorage": "^0.3.11",
    "tailwindcss": "^1.2.0",
    "xlsx": "^0.15.5"
  },
  "devDependencies": {
    "electron": "^9.4.0",
    "electron-packager": "^13.1.1"
  }
}
